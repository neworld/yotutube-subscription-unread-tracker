{
    "manifest_version": 2,
    "name": "Youtube subscription unread tracker",
    "description": "This plugin helps track new content in youtube subscription page.",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "http://www.youtube.com/*"
            ],
            "js": [
                "jquery-3.4.1.min.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}
